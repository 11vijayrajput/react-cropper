{
  "name": "react-cropper",
  "version": "1.3.0",
  "description": "Cropper as React Component",
  "main": "dist/react-cropper.umd.js",
  "module": "dist/react-cropper.es.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./example",
    "build": "rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "prepare": "npm run clean && npm run lint",
    "build:example": "npm run build && webpack --config webpack.config.js",
    "dev": "npm run build && node server.js",
